{
    "display": "Node 16",
    "_version": "16.1.0",
    "compilerOptions": {
        "lib": ["es2021"],
        "module": "Node16",
        "target": "es2021",
        "moduleResolution": "node16",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "baseUrl": "."
    },
    "include": ["src"]
}
